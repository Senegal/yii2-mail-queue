{
  "name": "dimichspb/yii2-mail-queue",
  "description": "Yii2 mail queue component",
  "keywords": ["yii2", "extension", "mail", "email", "queue"],
  "homepage": "https://github.com/dimichspb/yii2-mail-queue",
  "type": "yii2-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "Dmitry Tarantin",
      "email": "dimichspb@gmail.com"
    }
  ],
  "require": {
    "yiisoft/yii2": "*",
    "yiisoft/yii2-queue": "*",
    "yiisoft/yii2-swiftmailer": "*",
    "beberlei/assert": "^2.8",
    "ramsey/uuid": "^3.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^6"
  },
  "autoload": {
    "psr-4": {
      "dimichspb\\yii\\mailqueue\\": "src/",
      "dimichspb\\yii\\mailqueue\\tests\\": "tests/"
    }
  },
  "extra": {
    "bootstrap": "dimichspb\\yii\\mailqueue\\Bootstrap"
  }
}
